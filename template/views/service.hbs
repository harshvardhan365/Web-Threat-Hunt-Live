        class="show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center">
        <div class="spinner-border text-primary" role="status" style="width: 3rem; height: 3rem;"></div>
    </div>
    <!-- Spinner End -->


    <!-- Topbar Start -->
    {{>topbar}}
    <!-- Topbar End -->


    <!-- Navbar Start -->
      {{>navbar}}
    
    <!-- Navbar End -->


    <!-- Page Header Start -->
    {{!-- <div class="container-fluid page-header py-5 mb-5 wow fadeIn" data-wow-delay="0.1s"> --}}
    <div class="container-fluid page-header py-5 mb-5 wow fadeIn" data-wow-delay="0.1s">
        <div class="container text-center py-3">
            <h1 class="display-2 text-white mb-4 animated slideInDown">Services</h1>
            <nav aria-label="breadcrumb animated slideInDown">
                <ol class="breadcrumb justify-content-center mb-0">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>

                    <li class="breadcrumb-item text-primary" aria-current="page">Services</li>
                </ol>
            </nav>
        </div>
    </div>
    <!-- Page Header End -->
      <!-- Features Start -->
    <div class="container-xxl py-5">
      <div class="container">
        <div class="row g-0 feature-row" style="background-color: rgb(255, 255, 255);">
          <div class="col-md-6 col-lg-3 wow fadeIn" data-wow-delay="0.1s">
            <div class="feature-item border h-100 p-5">
              <div class="btn-square bg-light rounded-circle mb-4" style="width: 64px; height: 64px;">
                <img class="img-fluid" src="img/icon/icon-1.png" alt="Icon">
              </div>
              <h5 class="mb-3">Crowdsourced</h5>
              <p class="mb-0"> Harness the power of a global community to uncover and report vulnerabilities.</p>
            </div>
          </div>
          <div class="col-md-6 col-lg-3 wow fadeIn" data-wow-delay="0.3s">
            <div class="feature-item border h-100 p-5">
              <div class="btn-square bg-light rounded-circle mb-4" style="width: 64px; height: 64px;">
                <img class="img-fluid" src="img/icon/icon-2.png" alt="Icon">
              </div>
              <h5 class="mb-3">Transparent</h5>
              <p class="mb-0"> Ensure transparency by providing clear report of vulnerabilities and it's solutions.</p>
            </div>
          </div>
          <div class="col-md-6 col-lg-3 wow fadeIn" data-wow-delay="0.5s">
            <div class="feature-item border h-100 p-5">
              <div class="btn-square bg-light rounded-circle mb-4" style="width: 64px; height: 64px;">
                <img class="img-fluid" src="img/icon/icon-3.png" alt="Icon">
              </div>
              <h5 class="mb-3">Rewarding</h5>
              <p class="mb-0"> Platform offering bounties for identifying and responsibly disclosing security flaws.</p>
            </div>
          </div>
          <div class="col-md-6 col-lg-3 wow fadeIn" data-wow-delay="0.7s">
            <div class="feature-item border h-100 p-5">
              <div class="btn-square bg-light rounded-circle mb-4" style="width: 64px; height: 64px;">
                <img class="img-fluid" src="img/icon/icon-4.png" alt="Icon">
              </div>
              <h5 class="mb-3">Collaborative</h5>
              <p class="mb-0">A collaborative environment where researchers and developers can work together.</p>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- Features End -->


        <!-- Service Start -->
    <div class="container-xxl py-5">
      <div class="container">
        <div class="text-center mx-auto wow fadeInUp" data-wow-delay="0.1s" style="max-width: 500px;">
          <p class="fs-5 fw-medium text-primary">Our Services</p>
          <h1 class="display-5 mb-5">Service Provided By Web Threat Hunting</h1>
        </div>
        <div class="row g-4">
          <div class="col-lg-4 col-md-6 wow fadeInUp" data-wow-delay="0.1s">
            <div class="service-item position-relative h-100">
              <div class="service-text rounded p-5">
                <div class="btn-square bg-light rounded-circle mx-auto mb-4" style="width: 64px; height: 64px;">
                  <img class="img-fluid" src="img/icon/icon-5.png" alt="Icon">
                </div>
                <h5 class="mb-3">CyberCrime Hunter  </h4>
                  <p class="mb-0">Unleashing the digital detective within to protect against cyber threats and hunt down cyber criminals, these cyber crime hunters safeguard digital ecosystems and protect individuals.</p>
              </div>
              <div class="service-btn rounded-0 rounded-bottom">
                <a class="text-primary fw-medium" href="https://www.crowdstrike.com/cybersecurity-101/threat-hunting/">Learn More<i class="bi bi-chevron-double-right ms-2"></i>
                </a>
              </div>
            </div>
          </div>
          <div class="col-lg-4 col-md-6 wow fadeInUp" data-wow-delay="0.3s">
            <div class="service-item position-relative h-100">
              <div class="service-text rounded p-5">
                <div class="btn-square bg-light rounded-circle mx-auto mb-4" style="width: 64px; height: 64px;">
                  <img class="img-fluid" src="img/icon/icon-6.png" alt="Icon">
                </div>
                <h5 class="mb-3">Malware Analytics </h4>
                  <p class="mb-0">Malware Analytics: Unveiling insights and patterns within malicious software to enhance detection, prevention, and response, Harnessing data-driven insights to detect analyze.</p>
              </div>
              <div class="service-btn rounded-0 rounded-bottom">
                <a class="text-primary fw-medium" href="https://www.cisco.com/c/en/us/products/security/threat-grid/index.html">Learn More<i class="bi bi-chevron-double-right ms-2"></i>
                </a>
              </div>
            </div>
          </div>
          <div class="col-lg-4 col-md-6 wow fadeInUp" data-wow-delay="0.5s">
            <div class="service-item position-relative h-100">
              <div class="service-text rounded p-5">
                <div class="btn-square bg-light rounded-circle mx-auto mb-4" style="width: 64px; height: 64px;">
                  <img class="img-fluid" src="img/icon/icon-7.png" alt="Icon">
                </div>
                <h5 class="mb-3">Thick-App Pentesting </h4>
                  <p class="mb-0">Thick-App Pentesting involves assessing the security vulnerabilities of locally installed and resource-intensive applications to ensure their robustness against potential cyber threats.</p>
              </div>
              <div class="service-btn rounded-0 rounded-bottom">
                <a class="text-primary fw-medium" href="https://www.cyberark.com/resources/threat-research-blog/thick-client-penetration-testing-methodology">Learn More<i class="bi bi-chevron-double-right ms-2"></i>
                </a>
              </div>
            </div>
          </div>
          <div class="col-lg-4 col-md-6 wow fadeInUp" data-wow-delay="0.1s">
            <div class="service-item position-relative h-100">
              <div class="service-text rounded p-5">
                <div class="btn-square bg-light rounded-circle mx-auto mb-4" style="width: 64px; height: 64px;">
                  <img class="img-fluid" src="img/icon/icon-8.png" alt="Icon">
                </div>
                <h5 class="mb-3">Api Pentesting </h4>
                  <p class="mb-0">API Pentesting is a security assessment technique that identifies vulnerabilities and weaknesses in application programming interfaces to enhance their resilience against potential attacks.</p>
              </div>
              <div class="service-btn rounded-0 rounded-bottom">
                <a class="text-primary fw-medium" href="https://book.hacktricks.xyz/network-services-pentesting/pentesting-web/web-api-pentesting">Learn More <i class="bi bi-chevron-double-right ms-2"></i>
                </a>
              </div>
            </div>
          </div>
          <div class="col-lg-4 col-md-6 wow fadeInUp" data-wow-delay="0.3s">
            <div class="service-item position-relative h-100">
              <div class="service-text rounded p-5">
                <div class="btn-square bg-light rounded-circle mx-auto mb-4" style="width: 64px; height: 64px;">
                  <img class="img-fluid" src="img/icon/icon-9.png" alt="Icon">
                </div>
                <h5 class="mb-3">Ransomware Protection </h4>
                  <p class="mb-0">Ransomware Protection: Safeguarding against malicious software that encrypts and holds digital data hostage, ensuring data integrity and preventing extortion, ensuring your valuable information.</p>
              </div>
              <div class="service-btn rounded-0 rounded-bottom">
                <a class="text-primary fw-medium" href="https://www.ibm.com/ransomware">Learn More<i class="bi bi-chevron-double-right ms-2"></i>
                </a>
              </div>
            </div>
          </div>
          <div class="col-lg-4 col-md-6 wow fadeInUp" data-wow-delay="0.5s">
            <div class="service-item position-relative h-100">
              <div class="service-text rounded p-5">
                <div class="btn-square bg-light rounded-circle mx-auto mb-4" style="width: 64px; height: 64px;">
                  <img class="img-fluid" src="img/icon/icon-10.png" alt="Icon">
                </div>
                <h5 class="mb-3">Spam Email Filters</h4>
                  <p class="mb-0">Spam email filters: Shielding your inbox from unwanted messages and keeping it clean,Defending against digital clutter, spam email filters ensure a clean inbox, freeing you from irrelevant messages</p>
              </div>
              <div class="service-btn rounded-0 rounded-bottom">
                <a class="text-primary fw-medium" href="https://www.techtarget.com/searchsecurity/definition/spam-filter">Learn More<i class="bi bi-chevron-double-right ms-2"></i>
                </a>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- Service End -->


